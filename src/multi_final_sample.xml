	<entry>
	  <prototype>Mine</prototype>
	  <number>1</number>
	</entry>
	<entry>
	  <prototype>Enrichment</prototype>
	  <number>1</number>
	</entry>
	<entry>
 	  <prototype>LEU</prototype>
	 <number>1</number>
	</entry>
      </initialfacilitylist>
      <config>
	<StateInst>
          <declared_protos>
	    <val>Mine</val>
	    <val>Enrichment</val>
	    <val>LEU</val>
          </declared_protos>
          <secret_protos>
	    <val>Secret_Enrich</val>
	    <val>Secret_Sink</val>
          </secret_protos>
	  <weapon_status>0 </weapon_status>
	  <pursuit_factors>
	    <item>
	      <factor>Auth</factor>
	      <function>
		<name>Constant</name>
		<params>
		  <val>1</val>
		</params>
	      </function>
	    </item>
	    <item>
	      <factor>Enrich</factor>
	      <function>
		<name>Linear</name>
		<params>
		  <val>0</val>
		  <val>0.1</val>
		</params>
	      </function>
	    </item>
	    <item>
	      <factor>Mil_Iso</factor>
	      <function>
		<name>Constant</name>
