<simulation>
  <control>
    <duration>10</duration>
    <startmonth>1</startmonth>
    <startyear>2000</startyear>
  </control>
  
  <region>
    <name>SuperRegion</name>
    <config>
      <InteractRegion>
	<symmetric>1</symmetric>
	<pursuit_weights>
	  <item><factor>Auth</factor>      <weight>0.15</weight> </item>
	  <item><factor>Enrich</factor>    <weight>0.16</weight> </item>
	  <item><factor>Mil_Iso</factor>   <weight>0.10</weight> </item>
	  <item><factor>Mil_Sp</factor>    <weight>0.15</weight> </item>
	  <item><factor>Reactors</factor>  <weight>0.10</weight> </item>
	  <item><factor>U_Reserve</factor> <weight>0.09</weight> </item>
	  <item><factor>Sci_Net</factor>   <weight>0.10</weight> </item>
	  <item><factor>Conflict</factor>  <weight>0.15</weight> </item>
	</pursuit_weights>
	<likely_converter>  
	    <item>
	      <phase>Pursuit</phase>
	      <function>
		<name>power</name>
		<params>
		  <val>4</val>
		  <val>0.1</val>
		</params>
	      </function>
	    </item>
	    <item>
	      <phase>Acquire</phase>
	      <function>
		<name>Linear</name>
		<params>
		  <val>5.0</val>
		  <val>0.0</val>
		</params>
	      </function>
	    </item>
	</likely_converter>  
	<p_conflict_relations>  
	  <item>
	    <primary_state>StateA</primary_state>
	    <pair_state>
	      <item>
		<name>StateB</name>
		<relation>-1</relation>
	      </item>
	    </pair_state>
	  </item>
	  <item>
	    <primary_state>StateA</primary_state>
	    <pair_state>
	      <item>
		<name>StateC</name>
		<relation>-1</relation>
	      </item>
	    </pair_state>
	  </item>
	  <item>
	    <primary_state>StateB</primary_state>
	    <pair_state>
	      <item>
		<name>StateA</name>
		<relation>-1</relation>
	      </item>
	    </pair_state>
	  </item>
	  <item>
	    <primary_state>StateB</primary_state>
	    <pair_state>
	      <item>
		<name>StateC</name>
		<relation>-1</relation>
	      </item>
	    </pair_state>
	  </item>
	</p_conflict_relations>  
      </InteractRegion>
    </config>
    <institution>
